{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"module": "Node16",
		"outDir": ".",
		"sourceMap": true,
		"strict": false,
		"target": "ES2022"
	},
	"include": [
		"src/**/*.ts",
	],
	"exclude": [
		"node_modules",
	]
}
